{
    "class": "AS3",
    "action": "{{ action }}",
    "persist": true,
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.39.0",
        "id": "nginxCalalangNetWIP",
        "nginxCalalangNetWIP": {
            "class": "Tenant",
            "Shared": {
                "class": "Application",
                "template": "shared",
                "nginxDomain": {
                    "class": "GSLB_Domain",
                    "domainName": "nginx.calalang.net",
                    "aliases": [
                        "nginxcontroller.calalang.net",
                        "nginxplus.calalang.net"
                    ],
                    "resourceRecordType": "A",
                    "poolLbMode": "ratio",
                    "pools": [
                        {
                            "use": "nginxCalalangNetPool"
                        }
                    ]
                },
                "nginxCalalangNetPool": {
                    "class": "GSLB_Pool",
                    "enabled": true,
                    "lbModeAlternate": "ratio",
                    "lbModeFallback": "ratio",
                    "manualResumeEnabled": true,
                    "verifyMemberEnabled": false,
                    "qosHitRatio": 10,
                    "qosHops": 11,
                    "qosKbps": 8,
                    "qosLinkCapacity": 35,
                    "qosPacketRate": 5,
                    "qosRoundTripTime": 75,
                    "qosTopology": 3,
                    "qosVirtualServerCapacity": 2,
                    "qosVirtualServerScore": 1,
                    "members": [
                        {
                            "ratio": 1,
                            "server": {
                                "bigip": "/Common/calalang-azure-0"
                            },
                            "virtualServer": "/nginxCalalangNet/nginx.calalang.net/nginxCalalangNet_vs",
                            "dependsOn": "none"
                        }
                    ],
                    "bpsLimit": 5,
                    "bpsLimitEnabled": true,
                    "ppsLimit": 4,
                    "ppsLimitEnabled": true,
                    "connectionsLimit": 3,
                    "connectionsLimitEnabled": true,
                    "maxAnswersReturned": 10,
                    "monitors": [
                        {
                            "bigip": "/Common/http"
                        }
                    ],
                    "resourceRecordType": "A",
                    "fallbackIP": "1.1.1.1"
                }
            }
        }
    }
}