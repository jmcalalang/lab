{
    "class": "Telemetry",
    "controls": {
        "class": "Controls",
        "logLevel": "debug"
    },
    "Poller": {
        "class": "Telemetry_System_Poller",
        "interval": 60,
        "enable": true,
        "trace": false,
        "allowSelfSignedCert": false,
        "host": "localhost",
        "port": 8100,
        "protocol": "http"
    },
    "Beacon_Consumer": {
        "class": "Telemetry_Consumer",
        "type": "Generic_HTTP",
        "host": "ingestion.ovr.prd.f5aas.com",
        "protocol": "https",
        "port": 50443,
        "path": "/beacon/v1/ingest-telemetry-streaming",
        "method": "POST",
        "enable": true,
        "trace": false,
        "headers": [
            {
                "name": "grpc-metadata-x-f5-ingestion-token",
                "value": "`>@/passphrase`"
            }
        ],
        "passphrase": {
            "cipherText": "{{F5BeaconAccessToken}}"
        }
    },
    "schemaVersion": "1.0.0"
}