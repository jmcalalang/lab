apiVersion: k8s.nginx.org/v1
kind: GlobalConfiguration
metadata:
  name: nginx-configuration
  namespace: nginx-ingress
spec:
  listeners:
    - name: dns-udp
      port: 5353
      protocol: UDP
    - name: dns-tcp
      port: 5353
      protocol: TCP
    - name: "8888-tcp"
      port: 8888
      protocol: TCP
